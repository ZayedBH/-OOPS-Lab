import java.util.*;

class largest{
    public static void main(String[] args)
    {
        Scanner s = new Scanner(System.in);
        System.out.print("Enter the number of values");
        int n = s.nextInt();
        int sum =0,int num;
        System.out.print("Enter number ");
	      for (int i = 0; i < n; i++) 
	          {
              int num = s.nextInt();
              sum = sum + num
            }
        System.out.println("The largest number is: " + largest);
        s.close();
    }
}
